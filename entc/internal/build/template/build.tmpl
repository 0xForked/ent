{{ define "main" }}
package main

import (
	{{ printf "%q" .Package }}

	"fbc/ent"
)

{{ $base := base .Package}}

// {{ .Symbol }} is the plugin for exporting the {{ printf "%q" $base }} schemas for entc.
var {{ .Symbol }} = []ent.Schema{
	{{ range $_, $name := .Names }}
		{{- $base }}.{{- printf "%s{Schema: ent.DefaultSchema}" $name }},
	{{ end -}}
}
{{ end }}